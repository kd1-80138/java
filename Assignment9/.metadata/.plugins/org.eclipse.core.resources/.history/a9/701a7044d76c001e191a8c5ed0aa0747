package com.sunbeam;
import java.util.*;

public class Demoq1 {
	
	public static <T  extends Number> T minFromArray(T[] arr)
	{  T min=arr[0];
			for(int i=0;i<arr.length;i++)
			{
				if (min.doubleValue()>arr[i].doubleValue())
				{
					min= arr[i];
					}	
			}
			return T;
					
	}
	
	
	public static void main(String[] args)
	{
		Integer arr[]= {11,12,10,13,14,15};
		minFromArray(arr);
		Integer[] arr1 = { 22, 55, 77, 11, 33 };
		Integer min1 = findMin(arr1);
		Double[] arr2 = { 2.2, 5.5, 7.7, 1.1, 3.3 };
		Double min2 = findMin(arr2);
		
		
		
	}

}
