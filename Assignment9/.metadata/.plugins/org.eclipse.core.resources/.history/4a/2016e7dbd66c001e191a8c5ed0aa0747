package com.sunbeam;
import java.util.*;

public class Demoq1 {
	
	public static <T  extends Number> void minFromArray(T[] arr)
	{  T min=arr[0];
			for(int i=0;i<arr.length;i++)
			{
				if (min.doubleValue()>arr[i].doubleValue())
				{
					min= arr[i];
					}	
			}
			System.out.println("min element = "+min);
					
	}
	
	
	public static void main(String[] args)
	{
		Integer arr[]= {11,12,10,13,14,15};
		minFromArray(arr);
		
		
		
	}

}
