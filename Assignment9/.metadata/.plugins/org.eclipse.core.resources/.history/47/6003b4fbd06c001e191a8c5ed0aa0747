package com.sunbeam;
import java.util.*;

public class Demoq1 {
	
	public static <T  extends Comparable> void minFromArray(T[] arr)
	{  T min=arr[0];
			for(int i=0;i<arr.length;i++)
			{
				if (min.compareTo(arr[i])>0)
				{
					min=arr[i];
				}
				
			}
			System.out.println(min);
		
	}
	
	
	public static void main(String[] args)
	{
		Integer arr[]= {10,11,12,13,14,15};
		
		
		
	}

}
