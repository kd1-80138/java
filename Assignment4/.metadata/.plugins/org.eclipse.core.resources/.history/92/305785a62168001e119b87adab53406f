package com.app.fruits;

public class Mango extends Fruit{
	private String name ;
	private String taste;
	
	public Mango(String colour, double weight, boolean isFresh, String name, String taste) {
		super(colour, weight, isFresh);
		this.name = name;
		this.taste = taste;
	}

	public Mango() {
		super();
		this.name="Mango";
		this.taste="sweet";
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getTaste() {
		return taste;
	}

	public void setTaste(String taste) {
		this.taste = taste;
	}
	
	public String  taste()
	{
		return "Taste is Sweet";
	}
	
	
	
	

}
